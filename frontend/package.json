		"vite": "^5.4.11"
	},
	"type": "module",
	"dependencies": {
		"@fontsource/montserrat": "^5.0.18",
		"@fontsource/noto-sans-mono": "^5.0.20",
		"@fontsource/open-sans": "^5.0.28",
		"@tailwindcss/container-queries": "^0.1.1",
		"@tsparticles/slim": "^3.5.0",
		"@tsparticles/svelte": "^3.1.1",
		"shiki": "^1.14.1",
		"svelte-copy": "^2.0.0",
		"svelte-lazy": "^1.2.7",
		"tsparticles": "^3.7.1"
	}
}
